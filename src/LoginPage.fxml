<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginPageController">
   <children>
      <Pane prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: #000000;">
         <children>
            <Label alignment="CENTER" layoutX="281.0" layoutY="164.0" text="Welcome to " textAlignment="CENTER" textFill="#f3f3f3">
               <font>
                  <Font name="Martel" size="35.0" />
               </font>
            </Label>
            <Label layoutX="292.0" layoutY="267.0" text="SecStore" textFill="#f3f3f3">
               <font>
                  <Font name="Ubuntu Italic" size="50.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane layoutX="800.0" prefHeight="800.0" prefWidth="480.0" style="-fx-background-color: #4c4c4c;">
         <children>
            <Label layoutX="52.0" layoutY="274.0" text="Username:" textFill="#f3f3f3" />
            <TextField fx:id="txtUsername" layoutX="52.0" layoutY="289.0" prefHeight="25.0" prefWidth="365.0" style="-fx-background-color: #4c4c4c; -fx-text-fill: white;" />
            <Separator fx:id="sepUsername" layoutX="52.0" layoutY="314.0" prefHeight="2.0" prefWidth="365.0" style="-fx-base: #bebebe;" />
            <Separator fx:id="sepPassword" layoutX="52.0" layoutY="395.0" prefHeight="5.0" prefWidth="365.0" style="-fx-base: #bebebe;" />
            <Label layoutX="52.0" layoutY="355.0" text="Password:" textFill="#f3f3f3" />
            <PasswordField fx:id="txtPassword" layoutX="52.0" layoutY="370.0" prefHeight="25.0" prefWidth="365.0" style="-fx-background-color: #4c4c4c; -fx-text-fill: white;" />
            <Button fx:id="btnLogin" layoutX="327.0" layoutY="428.0" mnemonicParsing="false" onAction="#btnLogin_Pressed" prefHeight="39.0" prefWidth="90.0" style="-fx-background-color: #33b5e5;" text="Log In" textFill="#f3f3f3">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Hyperlink fx:id="btnRegister" layoutX="198.0" layoutY="490.0" style="-fx-text-fill: #33b5e5;" text="Register Now">
               <font>
                  <Font size="15.0" />
               </font>
            </Hyperlink>
         </children></Pane>
   </children>
</AnchorPane>
